{"ast":null,"code":"import Cliente from './services/clientes';\nexport default {\n  data() {\n    return {\n      cliente: {\n        documento: '',\n        nome: ''\n      },\n      clientes: []\n    };\n  },\n\n  mounted() {\n    Cliente.listar().then(response => {\n      this.clientes = response.data;\n    });\n  },\n\n  methods: {\n    salvar() {\n      Cliente.salvar(this.cliente).then(response => {\n        console.log(response.data);\n        alert('Cliente salvo com sucesso!');\n      });\n    },\n\n    deletar(cliente) {\n      Cliente.deletar(cliente).then(response => {\n        this.listar();\n        var error = response.data.errors;\n        console.log(error);\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAuDA,OAAOA,OAAP,MAAoB,qBAApB;AAEA,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,OAAO,EAAE;QACPC,SAAS,EAAE,EADJ;QAEPC,IAAI,EAAE;MAFC,CADJ;MAKLC,QAAQ,EAAE;IALL,CAAP;EAOD,CATY;;EAWbC,OAAO,GAAE;IACPN,OAAO,CAACO,MAAR,GAAiBC,IAAjB,CAAsBC,QAAO,IAAK;MAChC,KAAKJ,QAAL,GAAgBI,QAAQ,CAACR,IAAzB;IACD,CAFD;EAGD,CAfY;;EAiBbS,OAAO,EAAC;IAENC,MAAM,GAAE;MACNX,OAAO,CAACW,MAAR,CAAe,KAAKT,OAApB,EAA6BM,IAA7B,CAAkCC,QAAO,IAAI;QAC3CG,OAAO,CAACC,GAAR,CAAYJ,QAAQ,CAACR,IAArB;QACAa,KAAK,CAAC,4BAAD,CAAL;MACD,CAHD;IAKD,CARK;;IAUNC,OAAO,CAACb,OAAD,EAAS;MACdF,OAAO,CAACe,OAAR,CAAgBb,OAAhB,EAAyBM,IAAzB,CAA8BC,QAAO,IAAK;QACxC,KAAKF,MAAL;QACA,IAAIS,KAAI,GAAIP,QAAQ,CAACR,IAAT,CAAcgB,MAA1B;QACAL,OAAO,CAACC,GAAR,CAAYG,KAAZ;MACD,CAJD;IAKF;;EAhBM;AAjBK,CAAf","names":["Cliente","data","cliente","documento","nome","clientes","mounted","listar","then","response","methods","salvar","console","log","alert","deletar","error","errors"],"sourceRoot":"","sources":["/home/eduardo/Desktop/vue-api/API-clientes/src/App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n\n    <nav>\n      <div class=\"nav-wrapper\">\n        <a href=\"#\" class=\"brand-logo center\">Cadastro de clientes</a>\n      </div>\n    </nav>\n\n    <div class=\"container\">\n      <form @submit.prevent=\"salvar\">\n\n